    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^5.1.3",
    "less": "^4.1.1",
    "less-vars-to-js": "^1.3.0",
    "lint-staged": "^10.5.4",
    "postcss": "^8.2.8",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.0",
    "rimraf": "^3.0.2",
    "stylelint": "^13.12.0",
    "stylelint-config-recommended": "^4.0.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.2",
    "vite": "^2.0.5",
    "vite-plugin-babel-import": "^2.0.2",
    "vite-plugin-imp": "^2.0.4"
  },
  "engines": {
    "node": "^12 || >=14"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,ts,tsx}": [
      "pretty-quick --staged",
      "eslint --fix --quiet",
      "git add"
    ],
    "*.{css,scss,less,pcss}": [
      "stylelint"
    ]
  }
}
